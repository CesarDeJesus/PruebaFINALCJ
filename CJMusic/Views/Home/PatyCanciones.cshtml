
@{
    ViewBag.Title = "PatyCanciones";
    var DOS = ViewBag.ListaArtista;
}
@Styles.Render("~/Content/CSS/CancionesDes.css")
<p style="padding-top:27px; height:62px;  width:100%;"></p>


@model List<string>

@using (Html.BeginForm("Envia",
                        "Home",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{


    <label for="file">Upload Image: </label>
    <input type="file" name="file" id="file" />
    <br> <br>

    <input type="submit" value="Upload Image" />
    <br><br>
    @ViewBag.Message
    <br>


    <h2>Documents List</h2>

    <table>
        <tr>
            <th>File Name</th>
            <th>Link</th>
        </tr>
        @for (var i = 0; i <= DOS.Count - 1; i++)
        {
            <tr>
                <td>@DOS[i].ToString() </td>
                <td>@Html.ActionLink("Download", "Download", new { ImageName = @DOS[i].ToString() }) </td>
            </tr>
        }
    </table>
}